select count(*) from customer_order_products where full_name in('Gary Jenkins','Fred Fontain');
select*from customer_order_products;
select*from customer_order_products where order_total between 100 and 200;
select full_name as naam from customer_order_products where order_total between 100 and 200;
select count(*) from customers left join orders on customers.customer_id=orders.customer_id;
select count(*) from customers inner join orders on customers.customer_id=orders.customer_id;
